<h1>Edit Post</h1>

<%= form_tag(user_post_path, multipart: true, method: "put") do %>

  <%= label_tag(:image, "Image:") %>
  <%= file_field_tag(:image) %>

  <%= label_tag(:title, "Title:") %>
  <%= text_field_tag(:title) %>

  <%= label_tag(:body, "Body:") %>
  <%= text_field_tag(:body) %>


  <%= submit_tag("Submit") %>
<% end %>